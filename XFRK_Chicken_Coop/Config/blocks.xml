			<property name="Path" value="solid"/>
			<property name="Collide" value="movement,rockets,sight,melee"/>
			<property name="LootList" value="194"/>
			<property name="CustomIcon" value="FRK_Coop_Full"/>
			<property name="DowngradeBlock" value="FRK_Coop"/>
		</block>
</append>

